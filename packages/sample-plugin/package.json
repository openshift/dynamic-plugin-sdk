{
  "name": "@monorepo/sample-plugin",
  "version": "0.0.0-fixed",
  "description": "Sample dynamic plugin project",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn webpack",
    "build-prod": "yarn clean && NODE_ENV=production yarn webpack",
    "lint": "yarn run -T eslint $INIT_CWD",
    "webpack": "node -r ts-node/register ./node_modules/.bin/webpack",
    "http-server": "http-server dist -p 9001 -c-1 --cors"
  },
  "devDependencies": {
    "@openshift/dynamic-plugin-sdk": "portal:../lib-core",
    "@openshift/dynamic-plugin-sdk-webpack": "portal:../lib-webpack",
    "@patternfly/react-core": "^4.202.16",
    "@patternfly/react-table": "^4.71.16",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "http-server": "^14.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "~4.4.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "installConfig": {
    "hoistingLimits": "dependencies"
  }
}
