{
  "name": "@openshift/dynamic-plugin-sdk-utils",
  "version": "3.0.0",
  "description": "Provides React focused dynamic plugin SDK utilities",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/openshift/dynamic-plugin-sdk.git",
    "directory": "packages/lib-utils"
  },
  "files": [
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/index.d.ts",
    "dist/index.css",
    "dist/build-metadata.json"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "yarn build",
    "prepublishOnly": "yarn test",
    "build": "rm -rf dist && yarn run -T rollup -c && yarn api-extractor",
    "lint": "yarn run -T eslint $INIT_CWD",
    "test": "yarn run -T test $INIT_CWD",
    "api-extractor": "yarn run -T api-extractor -c $INIT_CWD/api-extractor.json"
  },
  "peerDependencies": {
    "@openshift/dynamic-plugin-sdk": "^4.0.0",
    "@openshift/dynamic-plugin-sdk-extensions": "^1.3.0",
    "@patternfly/react-core": "^5.1.0",
    "@patternfly/react-icons": "^5.1.0",
    "@patternfly/react-styles": "^5.1.0",
    "@patternfly/react-table": "^5.1.0",
    "@patternfly/react-virtualized-extension": "^5.0.0",
    "react": "^17 || ^18",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^6.3.0",
    "react-virtualized": "^9.22.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "typesafe-actions": "^4.4.2",
    "uuid": "^8.3.2"
  }
}
